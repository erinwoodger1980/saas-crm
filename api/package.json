{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "description": "Joinery AI API",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "prebuild": "prisma generate",
    "build": "tsc",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "sh -c 'prisma migrate deploy || (echo "[prisma] initial deploy failed, resetting schema" && prisma migrate reset --force --skip-seed && prisma migrate deploy)'"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "googleapis": "^161.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.104.0",
    "stripe": "^19.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@prisma/internals": "^6.17.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.6.1",
    "prisma": "^6.17.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}