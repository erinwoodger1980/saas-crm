{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "description": "Joinery AI API",
  "packageManager": "pnpm@9.0.0",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "prebuild": "prisma generate",
    "build": "tsc",
    "test": "jest",
    "self:check-parse": "node dist/scripts/self_check_parse.js",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "node scripts/prisma-deploy.mjs",
    "prisma:resolve:ead": "prisma migrate resolve --rolled-back 20251021173644_early_adopter_feedback --schema ./prisma/schema.prisma",
    "seed-template": "node scripts/create-seed-template.mjs",
    "seed:followups": "ts-node --transpile-only src/scripts/seed-followup-templates.ts",
    "followups:analyse": "ts-node --transpile-only src/scripts/followups-analyse.ts",
    "postinstall": "node scripts/prisma-postinstall.mjs && npx puppeteer browsers install chrome",
    "test:supplier": "ts-node-dev --transpile-only scripts/test-supplier-parse.ts",
    "audit:prod": "ts-node --transpile-only scripts/prod_ml_audit.ts",
    "ttl:ml": "ts-node --transpile-only scripts/ttl-ml-telemetry.ts",
    "images:import": "tsx scripts/import_tenant_images.ts",
    "images:bootstrap": "tsx scripts/bootstrap_tenant_from_url.ts",
    "keywords:sync": "tsx scripts/update_keywords_from_ads.ts",
    "seo:tune": "tsx scripts/seo_tune.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.926.0",
    "@prisma/client": "^6.17.1",
    "@octokit/rest": "^21.0.2",
    "@sparticuz/chromium": "^133.0.0",
    "@types/nodemailer": "^7.0.3",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "fast-levenshtein": "^3.0.0",
    "google-ads-api": "^21.0.1",
    "googleapis": "^161.0.0",
    "imghash": "^1.1.0",
    "jsonwebtoken": "^9.0.2",
    "minimist": "^1.2.8",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.10",
    "openai": "^4.104.0",
    "playwright": "^1.56.1",
    "puppeteer": "^23.7.1",
    "sharp": "^0.34.5",
    "stripe": "^19.1.0",
    "undici": "^7.16.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@prisma/internals": "^6.17.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cheerio": "^0.22.35",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/minimist": "^1.2.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.6.1",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "prisma": "^6.17.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
